[battle]
attacking="@@actor zendt zijn troepen van @@village om @@defender (@@pl_defender) aan te vallen."
defending="Jouw dorp @@village wordt aangevallen door @@attacker (@@pl_attacker)."

[messages]
received="Je hebt een bericht ontvangen van @@player."

[runes]
received="Je hebt @@runes ontvangen van @@player."

[resources]
received="Je hebt @@resources ontvangen van @@player."

[system]
test="Dit is een test notificatie. Je proefkonijn was @@user."

[clan]
